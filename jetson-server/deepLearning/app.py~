import yoloV5Wrap.wrap as yolo
import sys
import cv2
import time

if __name__ == "__main__":
    img = cv2.imread(sys.argv[1])

    print("[*] Running inference...")
    t0 = time.time()
    boxes = yolo.inference(img)
    t1 = time.time()
    print("[*] Inference Done in %0.2f(s)" % (t1 - t0))

    yolo.save_img_inference("results.jpg", img, boxes)
